#include<stdio.h>
#include<iostream>
using namespace std;
int main()
{
 int m,n,i,flag=0;
  cout<<"enter an number:\n";
  cin>>n;
  m=n/2;
  for(i=2;i<=n/2;i++)
  {
    if (n%i==0)
    {
      cout<<"not prime";
      flag==1;
      break;
    }  
   
    if (flag==0)
    {
      cout<<"prime";
    }
  }
}
